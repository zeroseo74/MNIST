      output = model(data)\n","\n","          test_loss += criterion(output, target).item()\n","          pred = output.argmax(dim=1, keepdim=True)\n","          correct += pred.eq(target.view_as(pred)).sum().item()\n","\n","    test_loss /= len(test_loader.dataset)\n","\n","    print('\\nTest set: Average loss: {:.4f}, Accuracy: {}/{} ({:.0f}%)\\n'.\n","          format(test_loss, correct, len(test_loader.dataset),100. * correct / len(test_loader.dataset)))"],"metadata":{"id":"e3u6-Aw8Jia8","executionInfo":{"status":"ok","timestamp":1641293163033,"user_tz":-540,"elapsed":4,"user":{"displayName":"신영서","photoUrl":"https://lh3.googleusercontent.com/a/default-user=s64","userId":"12027250976685881116"}}},"execution_count":33,"outputs":[]},{"cell_type":"markdown","source":["# epoch별 Train & Test 확인"],"metadata":{"id":"SVGEZN_hvxAP"}},{"cell_type":"markdown","source":["## 단층"],"metadata":{"id":"gT63A51wtwjk"}},{"cell_type":"code","source":["# 모델 돌릴때마다 같이 돌려줘야 model.parameters()값이 초기화됨\n","criterion = nn.CrossEntropyLoss().to(device) # 내부적으로 소프트맥스 함수를 포함하고 있음\n","optimizer = torch.optim.Adam(linear.parameters(),lr=0.01)"],"metadata":{"id":"KdiWYsXssxdL","executionInfo":{"status":"ok","timestamp":1641293383607,"user_tz":-540,"elapsed":381,"user":{"displayName":"신영서","photoUrl":"https://lh3.googleusercontent.com/a/default-user=s64","userId":"12027250976685881116"}}},"execution_count":35,"outputs":[]},{"cell_type":"code","source":["for epoch in range(1, 11):\n","  \n","    train(linear, train_loader, optimizer, epoch, criterion)\n","    test(linear, test_loader, criterion)\n","#torch.save(model, './model.pt')"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"BSgm9DguKERh","executionInfo":{"status":"ok","timestamp":1641293452832,"user_tz":-540,"elapsed":68001,"user":{"displayName":"신영서","photoUrl":"https://lh3.googleusercontent.com/a/default-user=s64","userId":"12027250976685881116"}},"outputId":"31cb9ba3-89fb-48f4-ebeb-84c2d630c25a"},"execution_count":36,"out